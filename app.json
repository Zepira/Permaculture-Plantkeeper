{
  "expo": {
    "name": "Plantkeeper",
    "slug": "plantkeeper",
    "version": "1.0.0",
    "scheme": "plantkeeper-scheme",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5EBB91"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "378c4de2-c930-4b5e-af3b-c75633b0e87d"
      }
    },
    "owner": "zepira",
    "plugins": ["expo-router"]
  }
}
